{
    "type":"Book",
    "count":{{len .}},
    "results":[
    {{range $index,$e := .}}{{if $index}},
    {{end}}   {
        "Title":"{{$e.Title}}",
        "Catalog":"{{$e.CatalogTitle}}",
        "Author":"{{$e.Author}}",
        "Version":{{$e.Version}},
        "ID":{{$e.ID}}
        }{{end}}
    ]
}